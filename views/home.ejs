  <!-- Testimonial section -->

  <section class="testimonial-section" id="testimonials">

    <div id="testimonial-carousel" class="carousel slide" data-ride="false">
      <div class="carousel-inner">

        <% reviewData.forEach((reviewee,index) => { %>

          <% if (index === 0) { %>
            <div class="carousel-item active">
              <div class="card">
                <img class="testimonial-image img-fluid mx-auto d-block" src="/images/card-images4.jpeg" alt="John">
                <span><%= reviewee.firstname %> <%= reviewee.lastname %></span>
                <p class="title"><%= reviewee.title %> | <%= reviewee.company %></p>
                <div>
                  <% for( let index = 0; index < reviewee.rating; index++ ) { %>
                    <i class="fa-solid fa-star" style="color: #edb007;"></i>
                  <% } %>
                </div>
                <div class="social-icon">
                  <a href=<%= reviewee.facebook %>><i class="fab fa-facebook fa-2x"></i></a>
                  <a href=<%= reviewee.twitter %>><i class="fab fa-twitter fa-2x"></i></a>
                  <a href=<%= reviewee.facebook %>><i class="fab fa-linkedin fa-2x"></i></a>
                  <a href=<%= reviewee.github %>><i class="fab fa-github fa-2x"></i></a>
                </div>
              </div>
            </div>
          <% } else { %>
            <div class="carousel-item">
              <div class="card">
                <img class="testimonial-image img-fluid mx-auto d-block" src="/images/card-images4.jpeg" alt="John">
                <span><%= reviewee.firstname %> <%= reviewee.lastname %></span>
                <p class="title"><%= reviewee.title %> | <%= reviewee.company %></p>
                <div>
                  <% for( let index = 0; index < reviewee.rating; index++ ) { %>
                    <i class="fa-solid fa-star" style="color: #edb007;"></i>
                  <% } %>
                </div>
                <div class="social-icon">
                  <a href=<%= reviewee.facebook %>><i class="fab fa-facebook fa-2x"></i></a>
                  <a href=<%= reviewee.twitter %>><i class="fab fa-twitter fa-2x"></i></a>
                  <a href=<%= reviewee.facebook %>><i class="fab fa-linkedin fa-2x"></i></a>
                  <a href=<%= reviewee.github %>><i class="fab fa-github fa-2x"></i></a>
                </div>
              </div>
            </div>
          <% } %>
          

        <% }) %>

      <a class="carousel-control-prev" href="#testimonial-carousel" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon"></span>
      </a>
      <a class="carousel-control-next" href="#testimonial-carousel" role="button" data-slide="next">
        <span class="carousel-control-next-icon"></span>
      </a>
    </div>

  </section>

  <!-- Contact section -->
  <section class="contact-section" id="contact">
    <div class="container">

      <div class="form-container">

        <div class="form-header"><h2>Let's <span style="color: #F16529;">talk</span></h2></div>
        <form action="/" method="post">
    
          <div class="inner-container">
            <input type="text" class="contact-text" id="fname" name="firstname" placeholder="Your name.."><br>
      
            <input type="text" class="contact-text" id="lname" name="lastname" placeholder="Your last name.."><br>
            <input type="email" class="contact-text" id="email" name="email" placeholder="Email"><br>
        
            <textarea id="subject" name="subject" placeholder="Write message.." style="height:200px; width:100%; " class="contact-textarea"></textarea><br>
        
            <!-- <input type="submit" value="Submit"> -->
            <button type="submit" class="contact-btn">Send <i class="fa-regular fa-paper-plane fa-beat-fade" style="color: #ec6d18;"></i></button>
          </div>
      
        </form>  
      </div>

      <div class="contact-icon">
        <a href="#"><i class="fab fa-facebook fa-2x"></i></a>
        <a href="#"><i class="fab fa-twitter fa-2x"></i></a>
        <a href="#"><i class="fab fa-linkedin fa-2x"></i></a>
        <a href="#"><i class="fab fa-github fa-2x"></i></a>
      </div>

      <div class="contact-image">
        <img src="/images/contact-image1.png" alt="">
      </div>

    </div>
  </section>

<%- include('partials/footer') %>